<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.fragmentebi.fragments.Homefragment"
        tools:layout='@layout/home'
        android:label="homeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        tools:layout="@layout/dashboard"
        android:name="com.example.fragmentebi.fragments.DashboardFragment"
        android:label="dashboardFragment" />
    <fragment
        android:id="@+id/notificationFragment"
        tools:layout="@layout/notification"
        android:name="com.example.fragmentebi.fragments.NotificationFragment"
        android:label="notificationFragment" />
</navigation>